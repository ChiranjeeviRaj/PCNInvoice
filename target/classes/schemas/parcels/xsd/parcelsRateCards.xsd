<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="services">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="service"/>
        <xs:element ref="congestionCharges"/>
        <xs:element ref="fuelSurCharges"/>
      </xs:sequence>
      <xs:attribute name="ServiceProvider" use="required" type="xs:NCName"/>
      <xs:attribute name="ShippingAccountName" use="required"/>
      <xs:attribute name="londonSurcharge" use="required" type="xs:double"/>
      <xs:attribute name="maxWegightPrice" use="required" type="xs:double"/>
      <xs:attribute name="pickPrices" use="required" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="service">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="parcelPerRate"/>
        <xs:element ref="maxWeight"/>
        <xs:element ref="maxLength"/>
        <xs:element ref="clentPrice"/>
        <xs:element ref="ourPrice"/>
        <xs:element ref="surCharge"/>
      </xs:sequence>
      <xs:attribute name="ProviderServiceCode" use="required" type="xs:NMTOKEN"/>
      <xs:attribute name="id" use="required" type="xs:double"/>
      <xs:attribute name="providerService" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="parcelPerRate" type="xs:NCName"/>
  <xs:element name="maxWeight" type="xs:double"/>
  <xs:element name="maxLength" type="xs:double"/>
  <xs:element name="clentPrice" type="xs:double"/>
  <xs:element name="ourPrice" type="xs:double"/>
  <xs:element name="surCharge" type="xs:double"/>
  <xs:element name="congestionCharges">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="congestionCharge"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="congestionCharge">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="serviceType"/>
        <xs:element ref="consignmentPerKilo"/>
        <xs:element ref="pricePerWeights"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="serviceType" type="xs:NCName"/>
  <xs:element name="consignmentPerKilo" type="xs:double"/>
  <xs:element name="pricePerWeights">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="pricePerWeight"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="pricePerWeight">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="defaultMaxWeight"/>
        <xs:element ref="price"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="defaultMaxWeight" type="xs:double"/>
  <xs:element name="price" type="xs:double"/>
  <xs:element name="fuelSurCharges">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="fuelSurChargePer"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="fuelSurChargePer" type="xs:double"/>
</xs:schema>
